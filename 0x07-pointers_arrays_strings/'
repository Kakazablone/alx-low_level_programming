#include "main.h"

/**
  *_memset -  fills the first n bytes of the memory area pointed to by s
  *with the constant byte b
  *
  *@s: The string
  *@b: Constant
  *@n: number of bytes to be filled
  *
  *Return: Pointer to s
  */

char *_memset(char *s, char b, unsigned int n)
{
	unsigned int count = 0;

	while (count < n)
	{
		s[count] = b;
		count++;
	}

	return (s);
}
